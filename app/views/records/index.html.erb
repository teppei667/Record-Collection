<div class="container">
  <%= link_to "user", end_users_path %></br>
  <div>
    <%= form_with url: search_path, method: :get, local: true do |f| %>
      <%= f.text_field :keyword, value: @keyword %>
      <%= f.submit "検索" %>
    <% end %>
  </div>

  <% @records.each do |record| %>
    <%= link_to record_path(record.id) do %>
      <%= attachment_image_tag record, :image, fallback: "no-image.png", size:'200x200' %>
    <% end %>
  <% end %>
</div>
